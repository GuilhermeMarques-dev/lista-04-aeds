#include <stdio.h>

int potencia(int base, int expoente) {
    int resultado = 1;
    for (int i = 0; i < expoente; i++) {
        resultado = resultado * base;
    }
    return resultado;
}

int main() {
    int b, e;
    scanf("%d %d", &b, &e);
    printf("%d elevado a %d = %d\n", b, e, potencia(b, e));
    return 0;
}
